{
    "version": "@GTKSHARPMANIFESTVERSION@",
    "workloads": {
      "gtk": {
        "description": ".NET SDK workload for building GtkSharp applications.",
        "packs": [
          "GtkSharp.Sdk",
          "GtkSharp.Ref",
          "GtkSharp.Runtime"
        ]
      }
    },
    "packs": {
      "GtkSharp.Sdk": {
        "kind": "sdk",
        "version": "@VERSION@"
      },
      "GtkSharp.Ref": {
        "kind": "framework",
        "version": "@VERSION@"
      },
      "GtkSharp.Runtime": {
        "kind": "framework",
        "version": "@VERSION@"
      },
      "GtkSharp.Workload.Template.CSharp": {
        "kind": "template",
        "version": "@VERSION@"
      },
      "GtkSharp.Workload.Template.FSharp": {
        "kind": "template",
        "version": "@VERSION@"
      },
      "GtkSharp.Workload.Template.VBNet": {
        "kind": "template",
        "version": "@VERSION@"
      }
    }
  }