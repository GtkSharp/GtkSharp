<example>
            <code lang="C#">
                [DllImport("gnomeui-2")]
                static extern IntPtr gnome_about_get_type();
                                                                                                                        
                public static new GLib.GType GType {
                        get {
                                IntPtr raw_ret = gnome_about_get_type();
                                GLib.GType ret = new GLib.GType(raw_ret);
